<!-- ==============================================
    HEADER
    =============================================== -->
    <header id="home" class="jumbotron">
    
      <div class="container">
      
        <img class="img-responsive img-center header-logo" src="http://simpleicon.com/wp-content/uploads/map-marker-13.png" alt="">
      
        <div class="message-box clearfix">
        
          <div class="first-line">
            <p>Helping you </p>
          </div>  
          
          <div class="second-line flexslider home-slider">
          
            <ul class="slides">
            
              <li><p>find your</p></li>
              
              <li><p>way</p></li>
              
            </ul>
            
          </div><!--End home-slider -->
          
        </div><!--End message-box -->
        
      </div><!--End container -->
      
    </header><!--End header -->

    <!-- ==============================================
    EvenTS
    =============================================== -->
    <section id="services" class="padding-bottom bg-light">
    
      <div class="container">
          
        <div class="section-header scrollimation fade-up">
        
          <div class="section-icon"><class="fa fa-wrench"></i></div>
        
          <h1 class="section-title">Events</h1>
          
          <p class="section-description lead"></p>
          
          
        </div><!--End section-header -->
  <div class="row">
      <div class="center">
        <%= form_tag "/events/search", method: :post do %>
        <%= label_tag :search %>
        <%= text_field_tag :search %>
        <%= submit_tag "Search" %>
        <% end %>
      </div>


      
        <div class="row">

        <div class="col-sm-6 item text-center scrollimation scale-in d1">

            <div class="icon"><i class="fa fa-calender"></i></div>
            <h3>Events</h3>
            <% @events.each do |event| %>
              <h4><%= link_to event.name, "/events/#{event.id}" %></h4>
            <% end %>
        </div>


         
         

          <div class="col-sm-6 item text-center scrollimation scale-in d1">

            <div class="icon"><i class="fa fa-calender"></i></div>
            <h3>Bart Stations</h3>
            <% @stations.each do |station| %>
              <h4><%= link_to station.name, "/stations/#{station.id}" %></h4>
            <% end %>


    
            

          </div>


        </div><!--End row -->

        

    
        
      </div><!--End container -->
      
    </section><!--End event section-->

  <%= link_to "Add New Event", "events/new" %>


   
